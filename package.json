{
  "name": "build",
  "version": "0.1.0",
  "scripts": {
    "webpack": "webpack",
    "svgo": "svgo \"img\" --disable removeViewBox --enable removeDimensions --multipass",
    "stylelint": "stylelint \"src/css/**/*.css\"",
    "eslint": "eslint \"src/js/**/*.js\"",
    "lint": "run-s stylelint eslint"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "define-mixin",
            "mixin"
          ]
        }
      ]
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb-base",
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "what-input": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.2",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "npm-run-all": "^4.1.3",
    "nunjucks-html-loader": "^1.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^2.1.5",
    "postcss-mixins": "^6.2.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "svgo": "^1.0.5",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-notifier": "^1.6.0"
  }
}
